REG ${BASE} C${INDEX}FIFOCON${CHAN} CLR SET INV
BIT 20-16   FSIZE
BIT 14      FRESET
BIT 13      UINC
BIT 12      DONLY
BIT 7       TXEN
BIT 6       TXABAT
BIT 5       TXLARB
BIT 4       TXERR
BIT 3       TXREQ
BIT 2       RTREN
BIT 1-0     TXPRI

REG +{${BASE},10} C${INDEX}FIFOINT${CHAN} CLR SET INV
BIT 26      TXNFULLIE
BIT 25      TXHALFIE
BIT 24      TXEMPTYIE
BIT 19      RXOVFLIE
BIT 18      RXFULLIE
BIT 17      RXHALFIE
BIT 16      RXNEMPTYIE
BIT 10      TXNFULLIF
BIT 9       TXHALFIF
BIT 8       TXEMPTYIF
BIT 3       RXOVFLIF
BIT 2       RXFULLIF
BIT 1       RXHALFIF
BIT 0       RXNEMPTYIF

REG +{${BASE},20} C${INDEX}FIFOUA${CHAN} CLR SET INV
BIT 31-0    C1FIFOUA

REG +{${BASE},30} C${INDEX}FIFOCI${CHAN} CLR SET INV
BIT 4-0     C1FIFOCI
