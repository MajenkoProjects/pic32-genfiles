BIT 31-16   UNUSED
BIT 15      ${if ${PINS} == 100 ? LATD15 : UNUSED}
BIT 14      ${if ${PINS} == 100 ? LATD14 : UNUSED}
BIT 13      ${if ${PINS} == 100 ? LATD13 : UNUSED}
