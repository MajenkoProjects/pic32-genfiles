PERIPHERAL ${BASE} PORTD
REG ${rpn ${BASE} 0x00 +} TRISD CLR SET INV
BIT 31-16   UNUSED
BIT 15      ${if ${PINS} == 100 ? TRISD15 : UNUSED}
BIT 14      ${if ${PINS} == 100 ? TRISD14 : UNUSED}
BIT 13      ${if ${PINS} == 100 ? TRISD13 : UNUSED}
BIT 12      ${if ${PINS} == 100 ? TRISD12 : UNUSED}
BIT 11      TRISD11
BIT 10      TRISD10
BIT 9       TRISD9
BIT 8       TRISD8
BIT 7       TRISD7
BIT 6       TRISD6
BIT 5       TRISD5
BIT 4       TRISD4
BIT 3       TRISD3
BIT 2       TRISD2
BIT 1       TRISD1
BIT 0       TRISD0

REG ${rpn ${BASE} 0x10 +} PORTD CLR SET INV
BIT 31-16   UNUSED
BIT 15      ${if ${PINS} == 100 ? RD15 : UNUSED}
BIT 14      ${if ${PINS} == 100 ? RD14 : UNUSED}
BIT 13      ${if ${PINS} == 100 ? RD13 : UNUSED}
BIT 12      ${if ${PINS} == 100 ? RD12 : UNUSED}
BIT 11      RD11
BIT 10      RD10
BIT 9       RD9
BIT 8       RD8
BIT 7       RD7
BIT 6       RD6
BIT 5       RD5
BIT 4       RD4
BIT 3       RD3
BIT 2       RD2
BIT 1       RD1
BIT 0       RD0

