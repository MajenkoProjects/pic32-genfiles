BIT 25      TXHALFIE
BIT 24      TXEMPTYIE
BIT 23-20   UNUSED
BIT 19      RXOVFLIE
BIT 18      RXFULLIE
BIT 17      RXHALFIE
BIT 16      RXNEMPTYIE
BIT 11-15   UNUSED
BIT 10      TXNFULLIF
BIT 9       TXHALFIF
BIT 8       TXEMPTYIF
BIT 4-7     UNUSED
BIT 3       RXOVFLIF
BIT 2       RXFULLIF
BIT 1       RXHALFIF
BIT 0       RXNEMPTYIF
